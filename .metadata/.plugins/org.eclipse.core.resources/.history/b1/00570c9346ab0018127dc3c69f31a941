package dataStructures;

public class Arrays {

	public static void main(String[] args) {
		
		//Initializing arrays
		int[] numbers = {0, 1, 2, 3};
		int[] numbers2 = new int[5];
		
		//Find length of array
		System.out.println(numbers.length);
		System.out.println(numbers2.length);
		
		//Accessing elements in array
		
		
		
	}

}
